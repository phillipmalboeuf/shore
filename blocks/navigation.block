<nav>
  {.repeated section items}
    {.section collection}
      <a href="{fullUrl}" class="nav__link js-nav__link" data-id="{id}">{navigationTitle}</a>
    {.end}

    {.section externalLink}
      <a href="{url}" class="nav__link js-nav__link"{.section newWindow} target="_blank"{.end}>{title}</a>
    {.end}
  {.end}
</nav>