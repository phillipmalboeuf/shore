
{.section website}
<article class="box">
	<h1 class="h1--huge text_center">{title}</h1>
</article>
{.end}

{.section collection}
    <h1>{title}</h1>
    {.repeated section collections}
        <h2>{title}</h2>
        {.repeated section items}
            <ul>
                <li>{title}</li>
            </ul>
        {.end}
    {.end}
{.end}